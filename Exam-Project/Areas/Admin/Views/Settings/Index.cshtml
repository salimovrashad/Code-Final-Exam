@model List<Settings>
@{
    ViewData["Title"] = "Index";
}

<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-4 text-gray-800">Settings Page</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Address</th>
                <th colspan="2" scope="col">Handle</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Address</td>
                    <td><a class="btn btn-success" asp-action="Update" asp-controller="Settings" asp-route-id="@item.Id">UPDATE</a></td>
                </tr>
            }

        </tbody>
    </table>

</div>
<!-- /.container-fluid -->